{
  "footer#mini": {
    "blocks": ["footer-layout.desktop#mini", "footer-layout.mobile#mini"]
  },
  "footer-layout.desktop#mini": {
    "children": [
      "flex-layout.row#mini-footer-logos",
      "flex-layout.row#Footer-Xtrategik"
    ]
  },
  "flex-layout.row#footer-1-desktop#mini": {
    "children": [
      "flex-layout.row#Padre",
      "vtex.menu@2.x:menu#TerminosLegales",
      "vtex.menu@2.x:menu#LaTienda",
      "flex-layout.row#qualia-ubuntec-logos"
    ],
    "props": {
      "blockClass": "footerDesktop"
    }
  },

  "footer-layout.mobile#mini": {
    "children": ["flex-layout.row#mini-footer-mobile"]
  },
  "flex-layout.row#mini-footer-mobile": {
    "children": ["flex-layout.col#mini-footer-mobile-col"],
    "props": {
      "blockClass": "mini-footer-mobile",
      "fulwidth": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "center",
      "verticalAlign": "middle",
      "paddingTop": 0,
      "paddingBottom": 0
    }
  },

  "flex-layout.col#mini-footer-mobile-col": {
    "children": [
      "flex-layout.row#qualia-mini-footer-logo",
      "flex-layout.row#ubuntec-mini-footer-logo",
      "flex-layout.row#mini-footer-mobile-comercio-fintech-industrial-logos",
      "flex-layout.col#links-mini-footer",
      "flex-layout.col#numero-mini-footer",
      "flex-layout.col#mini-footer-whatsapp-logo"
    ],
    "props": {
      "blockClass": "mini-footer-mobile",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },

  "flex-layout.row#mini-footer-mobile-comercio-fintech-industrial-logos": {
    "children": [
      "flex-layout.row#fintech-mini-footer-logo",
      "flex-layout.row#camara-comercio-mini-footer-logo",
      "flex-layout.row#dinsutria-mini-footer-logo"
    ],
    "props": {
      "blockClass": "mini-footer-mobile-comercio-fintech-industrial-logos",
      "fulwidth": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "center",
      "verticalAlign": "middle",
      "paddingTop": 0,
      "paddingBottom": 0
    }
  },

