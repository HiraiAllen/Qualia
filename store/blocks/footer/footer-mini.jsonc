{
  "footer#mini": {
    "blocks": ["footer-layout.desktop#mini", "footer-layout.mobile#mini"]
  },
  "footer-layout.desktop#mini": {
    "children": [
      "flex-layout.row#mini-footer-logos",
      "flex-layout.row#Footer-Xtrategik"
    ]
