      // "rich-text#Hijo1",
      // "rich-text#Hijo2",
      // "rich-text#Hijo3",
      "flex-layout.row#socialMediaFooterIcons"
    ],
    "props": {
      "blockClass": "Hijo"
    }
  },
  "rich-text#contactTitle": {
    "props": {
      "text": "QUALIA: ",
      "blockClass": "contactTextTitle",
      "color": "white"
    }
  },
  "rich-text#Hijo1": {
