{
  "flex-layout.row#Padre": {
    "children": [
      "flex-layout.col#Hijo"
    ],
    "props": {
      "blockClass": "Padre",
      "width": "20%"
    }
  },
  "flex-layout.col#Hijo": {
    "children": [
      "vtex.menu@2.x:menu#Qualia",
