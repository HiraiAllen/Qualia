{
    
      "tab-layout#productos-servicios": {
        "children": [
          "tab-list#productos-servicios",
          "tab-content#productos-servicios"
        ],
        "props": {
          "blockClass": "home",
          "defaultActiveTabId": "home1"
        }
      },
      "tab-list#productos-servicios": {
        "children": [
          "tab-list.item#productos",
          "tab-list.item#servicios"
        ]
      },
      "tab-list.item#productos": {
        "props": {
          "tabId": "productos",
          "label": "Productos",
          "defaultActiveTab": true
        }
      },
      "tab-list.item#servicios": {
        "props": {
          "tabId": "servicios",
          "label": "Servicios"
        }
      },
    "tab-content#productos-servicios": {
      "children": [
        "tab-content.item#productos",
        "tab-content.item#servicios"
      ]
    },
    "tab-content.item#productos": {
      "children": [
        "flex-layout.row#shelf"
      ],
      "props": {
        "tabId": "productos"
      }
    },
    "tab-content.item#servicios": {
      "children": [
        "flex-layout.row#shelf"
      ],
      "props": {
        "tabId": "servicios"
      }
    },

    "rich-text#prueba-productos": {
        "props": {
                 "text": "Aqui van productos",
                 "blockClass": "class",
                 "textAlignment": "CENTER",
                 "textPosition": "CENTER",
                 "color": "black"
        }
    },

    "rich-text#prueba-servicios": {
        "props": {
                 "text": "Aqui van servicios",
                 "blockClass": "class",
                 "textAlignment": "CENTER",
                 "textPosition": "CENTER",
                 "color": "black"
        }
    }
}